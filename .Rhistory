pos_pred <- function(x) {
Sens <- 0.8
Spez <- 0.7
y <-    (Sens * x) / ((Sens * x) + (1 - Spez) * (1 - x))
paste0(" Das positive prädictive Wert für eine prävalenz von ", x, " ist: ",
y)
}
neg_pred <- function(x) {
Sens <- 0.8
Spez <- 0.7
(Spez * (1 - x)) / (Spez * (1 - x) + (1 - Sens) * x)
}
